{
  "name": "smell-o-tron-api",
  "version": "1.0.0",
  "description": "Perfume recommendation generator",
  "main": "server.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "nf start -p 3000 --procfile Procfile.dev",
    "clean-public": "npm run remove-public && mkdir public",
    "remove-public": "rimraf ./public",
    "build:html": "babel-node tools/buildHtml.js",
    "heroku-prebuild": "npm run clean-public && npm run build:html",
    "build": "babel-node tools/build.js",
    "heroku-postbuild": "babel-node tools/publicServer.js",
    "babel-version": "babel --version"

  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/christinechoi/smell-o-tron-api.git"
  },
  "keywords": [
    "perfume"
  ],
  "author": "christine choi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/christinechoi/smell-o-tron-api/issues"
  },
  "engines": {
    "node": "8.9.1"
  },
  "homepage": "https://github.com/christinechoi/smell-o-tron-api#readme",
  "dependencies": {
    "babel-cli": "6.8.0",
    "babel-polyfill": "6.8.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "bootstrap": "^3.3.7",
    "cheerio": "^0.20.0",
    "colors": "1.1.2",
    "compression": "^1.6.1",
    "express": "4.16.2",
    "gulp": "^3.9.1",
    "minimatch": "^3.0.4",
    "npm-run-all": "^4.1.2",
    "open": "0.0.5",
    "react": "^15.3.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.7.0",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0",
    "serve-favicon": "^2.3.0"
  }
}
